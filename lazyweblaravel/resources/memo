
/**************************************************************

Membership Management CRUD

# Common parameters:
        Member (Both new/existing) personal information
        Members' emergency correspondence information

**************************************************************/


GET /members/{uid}
{
    Param:
        {Authentication-Provider}
        {id}/{password} or {Authentication-Token}

    Description:
        Retrieves user information.
        ID/Password

    Response:


}




POST /members/{uid}
{
    Param:
        {Authentication-Provider}
        {id}/{password} or {Authentication-Token}

    Description:
        Creates new user account

    Response:


}




PUT /members/{uid}
{
    Param:
        {Authentication-Provider}
        {id}/{password} or {Authentication-Token}

    Description:
        Updates existing user information

    Response:


}


DELETE /members/{uid}
{
    Param:
        {Authentication-Provider}
        {id}/{password} or {Authentication-Token}

    Description:

    Response:


}




